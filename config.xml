<?xml version="1.0" encoding="utf-8"?>
<http://www.w3.org/ns/widgets:widget xmlns:null="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.linkses.certifica" version="1.0.4">
	<http://www.w3.org/ns/widgets:feature name="Whitelist">
		<http://www.w3.org/ns/widgets:param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
		<http://www.w3.org/ns/widgets:param name="onload" value="true" />
	</http://www.w3.org/ns/widgets:feature>
	<http://www.w3.org/ns/widgets:name />
	<http://www.w3.org/ns/widgets:description />
	<http://www.w3.org/ns/widgets:author email="info@certifica.email" href="https://www.certifica.email/" />
	<http://www.w3.org/ns/widgets:content src="index.html" />
	<http://www.w3.org/ns/widgets:access origin="*" />
	<http://www.w3.org/ns/widgets:allow-intent href="http://*/*" />
	<http://www.w3.org/ns/widgets:allow-intent href="https://*/*" />
	<http://www.w3.org/ns/widgets:allow-intent href="tel:*" />
	<http://www.w3.org/ns/widgets:allow-intent href="sms:*" />
	<http://www.w3.org/ns/widgets:allow-intent href="mailto:*" />
	<http://www.w3.org/ns/widgets:allow-intent href="geo:*" />
	<http://www.w3.org/ns/widgets:allow-intent href="market:*" />
	<http://www.w3.org/ns/widgets:icon src="www/img/icon.png" />
	<http://www.w3.org/ns/widgets:preference name="loglevel" value="DEBUG" />
</http://www.w3.org/ns/widgets:widget>
